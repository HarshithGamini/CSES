#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
 
void solve() {
    ll n;
    cin >> n;
    cout << 0 << "\n";
    for(ll k = 2;k <= n;k++) {
        cout << ((k * k) * (k * k - 1)) / 2 - (4 * (k - 2) * (k - 1)) << "\n";
    } 
}
 
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
 
    int t = 1;
    //cin >> t;
    while(t--) {
        solve();
    }
    return 0;
}
