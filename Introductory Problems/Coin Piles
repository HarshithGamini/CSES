#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
 
void solve() {
    int a, b;
    cin >> a >> b;
    if((2 * a - b) % 3 == 0 && (2 * a - b) >= 0 && (2 * b - a) % 3 == 0 && (2 * b - a) >= 0) {
        cout << "YES" << "\n";
    } else {
        cout << "NO" << "\n";
    }
}
 
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
 
    int t = 1;
    cin >> t;
    while(t--) {
        solve();
    }
    return 0;
}
