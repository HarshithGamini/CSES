#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
 
void solve() {
    ll n;
    cin >> n;
    ll sum = 0;
    for(ll i = 0;i < n - 1;i++) {
        ll x;
        cin >> x;
        sum += x;
    }
    cout << (n * (n + 1)) / 2 - sum << "\n";
}
 
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
 
    int t = 1;
    //cin >> t;
    while(t--) {
        solve();
    }
    return 0;
}
