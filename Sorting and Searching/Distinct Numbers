#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
 
void solve() {
    int n;
    cin >> n;
    vector<int> x(n);
    set<int> st;
    for(int i = 0;i < n;i++) {
        cin >> x[i];
        st.insert(x[i]);
    }
    cout << st.size() << "\n";
}
 
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
 
    int t = 1;
    //cin >> t;
    while(t--) {
        solve();
    }
    return 0;
}
